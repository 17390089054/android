	 */
	public void login(String account,String password){
		try {
			sqlSession=MyBatisUtil.getSqlSession();
			UserDao userDao = sqlSession.getMapper(UserDao.class);
			
			
		} catch (Exception e) {
			
		}finally{
			MyBatisUtil.closeSqlSession(sqlSession);
		}
	
	}
}
